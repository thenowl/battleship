{
  "extends": ["airbnb-base", "prettier"],
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "plugins": ["prettier"],
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": "warn"
  }
}
